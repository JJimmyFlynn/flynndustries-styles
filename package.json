{
  "name": "generator-flynndustries-styles",
  "version": "0.0.0",
  "description": "A simple Yeoman generator to generate SCSS files for projects",
  "homepage": "",
  "author": {
    "name": "John Flynn",
    "email": "JJimmyFlynn@gmail.com",
    "url": "https://johnjflynn.net"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.5.2",
    "cp-cli": "^1.0.2",
    "del-cli": "^1.1.0",
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^22.0.6"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "copy": "cp-cli -d src generators",
    "clean": "del generators",
    "compile": "babel -d generators/ --ignore templates/ generators/",
    "pretest": "npm run get-ready",
    "test": "standard; jest",
    "postversion": "git push; git push --tags; npm publish",
    "prepare": "npm run clean; npm run copy; npm run compile"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "JJimmyFlynn/generator-flynndustries-styles",
  "license": "MIT"
}
